{
    "name": "Innovate3DTheme",
    "author": "Innovate3D",
    "technicalName": "Innovate3DTheme",
    "description": "Custom theme for Innovate3D",
    "views": [
        "@Storefront",
        "@Plugins"
    ],
    "style": [
        "@Storefront",
        "app/storefront/src/scss/base.scss"
    ],
    "script": [
        "@Storefront",
        "app/storefront/src/main.js"
    ],
    "asset": [
        "@Storefront",
        "app/storefront/src/assets"
    ],
    "parent": "@Storefront",
    "configFields": {
        "sw-color-brand-primary": {
            "label": {
                "en-GB": "Primary colour",
                "de-DE": "Primärfarbe"
            },
            "value": "#0d0d0d"
        },
        "sw-color-brand-secondary": {
            "label": {
                "en-GB": "Secondary colour",
                "de-DE": "Sekundärfarbe"
            },
            "value": "#ffffff"
        },
        "sw-color-brand-accent": {
            "label": {
                "en-GB": "Accent colour",
                "de-DE": "Akzentfarbe"
            },
            "value": "#ff6b6b"
        }
    }
} 